{
  "task_id": "s2_case051",
  "description": "Sequential cooperative task: RobotA first moves the red box from Shelf.red.slot to RedBin.slot, then RobotB moves the blue box from Shelf.blue.slot to BlueBin.slot.",
  "goal": {
    "RedBin.slot": "redbox",
    "BlueBin.slot": "bluebox"
  },
  "steps": [
    {
      "agent": "robotA",
      "action": "base.goto",
      "target": "Shelf.front.dock"
    },
    {
      "agent": "robotA",
      "action": "arm.pick",
      "object": "redbox",
      "from": "Shelf.red.slot"
    },
    {
      "agent": "robotA",
      "action": "base.goto",
      "target": "RedBin.dock"
    },
    {
      "agent": "robotA",
      "action": "arm.place",
      "object": "redbox",
      "to": "RedBin.slot"
    },
    {
      "agent": "robotB",
      "action": "base.goto",
      "target": "Shelf.front.dock"
    },
    {
      "agent": "robotB",
      "action": "arm.pick",
      "object": "bluebox",
      "from": "Shelf.blue.slot"
    },
    {
      "agent": "robotB",
      "action": "base.goto",
      "target": "BlueBin.dock"
    },
    {
      "agent": "robotB",
      "action": "arm.place",
      "object": "bluebox",
      "to": "BlueBin.slot"
    }
  ]
}